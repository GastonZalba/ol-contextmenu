{
    "name": "ol-contextmenu",
    "version": "5.0.1-alpha",
    "description": "Custom Context Menu for Openlayers",
    "type": "module",
    "main": "./dist/ol-contextmenu.umd.cjs",
    "module": "./dist/ol-contextmenu.js",
    "exports": {
        ".": {
            "import": "./dist/ol-contextmenu.js",
            "require": "./dist/ol-contextmenu.umd.cjs"
        }
    },
    "types": "./dist/ol-contextmenu.d.ts",
    "author": "Jonatas Walker",
    "homepage": "https://github.com/jonataswalker/ol-contextmenu",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git://github.com/jonataswalker/ol-contextmenu.git"
    },
    "bugs": {
        "url": "https://github.com/jonataswalker/ol-contextmenu/issues"
    },
    "engines": {
        "node": "^16 || ^18",
        "npm": ">=8"
    },
    "files": [
        "dist/"
    ],
    "scripts": {
        "dev": "vite",
        "build": "npm run generate-types && tsc && vite build",
        "preview": "vite preview",
        "lint": "eslint --cache --ext .js .",
        "lint:fix": "npm run lint -- --fix",
        "generate-types": "tsc src/**/*.ts --declaration --declarationMap --emitDeclarationOnly --outdir dist",
        "test": "jest"
    },
    "devDependencies": {
        "@swc/core": "^1.3.0",
        "@swc/jest": "^0.2.22",
        "@types/jest": "^29.0.2",
        "@vitejs/plugin-legacy": "^2.1.0",
        "canvas": "^2.10.1",
        "eslint": "^8.23.1",
        "eslint-config-jwalker": "^7.9.2",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-jest": "^27.0.4",
        "eslint-plugin-prettier": "^4.2.1",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^29.0.3",
        "jest-environment-jsdom": "^29.0.3",
        "ol": "^7.1.0",
        "prettier": "^2.7.1",
        "typescript": "^4.8.3",
        "vite": "^3.1.0"
    },
    "peerDependencies": {
        "ol": "^7.1.0"
    },
    "dependencies": {
        "tiny-emitter": "^2.1.0"
    }
}
