{
    "name": "ol-contextmenu",
    "version": "4.1.0",
    "description": "Custom Context Menu for Openlayers",
    "type": "module",
    "main": "./dist/ol-contextmenu.umd.cjs",
    "module": "./dist/ol-contextmenu.js",
    "exports": {
        ".": {
            "import": "./dist/ol-contextmenu.js",
            "require": "./dist/ol-contextmenu.umd.cjs"
        }
    },
    "types": "./dist/ol-contextmenu.d.ts",
    "author": "Jonatas Walker",
    "homepage": "https://github.com/jonataswalker/ol-contextmenu",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git://github.com/jonataswalker/ol-contextmenu.git"
    },
    "bugs": {
        "url": "https://github.com/jonataswalker/ol-contextmenu/issues"
    },
    "engines": {
        "node": "^16 || ^18",
        "npm": ">=8"
    },
    "files": [
        "dist/"
    ],
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "lint": "eslint --cache --cache-location .eslintcache/ --ext .js .",
        "lint:fix": "npm run lint -- --fix",
        "generate-types": "tsc src/**/*.ts --declaration --declarationMap --emitDeclarationOnly --outdir dist",
        "test": "npm run build && npm run test:unit",
        "test:unit": "jest",
        "test:unit:watch": "jest --watch"
    },
    "devDependencies": {
        "@types/ol": "^6.5.3",
        "@vitejs/plugin-legacy": "^2.0.1",
        "canvas": "^2.9.3",
        "eslint": "^8.22.0",
        "eslint-config-jwalker": "^7.9.2",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "ol": "^7.0.0",
        "prettier": "^2.7.1",
        "rollup-plugin-terser": "^7.0.2",
        "typescript": "^4.7.4",
        "vite": "^3.0.9"
    },
    "peerDependencies": {
        "ol": "^6.15.1"
    },
    "dependencies": {
        "redom": "^3.29.0",
        "tiny-emitter": "^2.1.0"
    }
}
